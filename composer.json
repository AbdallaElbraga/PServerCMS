{
  "name": "kokspflanze/p-server-cms",
  "description": "Zendframework 2 Module for pServer ",
  "keywords": [
    "framework",
    "zf2",
    "game backend",
    "p-server-cms"
  ],
  "homepage": "https://github.com/kokspflanze/PServerCMS",
  "autoload": {
    "psr-0": {
      "PServerCMS\\": "src/",
      "PServerCMSTest\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Oliver",
      "email": "kokspflanze@gmx.de"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-intl" : "*",
    "ext-gd" : "*",
    "ext-curl" : "*",
    "twig/twig": "1.*",
    "doctrine/doctrine-orm-module": "0.*",
    "mabuzagu/pdo-dblib-module": "dev-master",
    "kokspflanze/zfc-ticket-system": "dev-master",
    "kokspflanze/small-user": "0.*",
    "kokspflanze/game-backend": "dev-master",
    "kokspflanze/p-server-admin": "dev-master",
    "kokspflanze/p-server-cli": "0.*",
    "kokspflanze/payment-api": "0.*",
    "kokspflanze/p-server-ranking": "dev-master",
    "kokspflanze/p-server-panel": "dev-master",
    "zf-commons/zfc-twig": "dev-master",
    "kokspflanze/zfc-bbcode": "~1.0",
    "san/san-captcha": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4|~5",
    "zendframework/zendframework": "~2.0",
    "fabpot/php-cs-fixer": ">=1.0",
    "satooshi/php-coveralls": "dev-master",
    "phpunit/phpcov": ">=2.0"
  },
  "suggest" : {
    "kokspflanze/p-server-sro": "Its a extension for Silkroad, to provide more features for that game"
  }
}
