    "framework",
    "zf2",
    "game backend",
    "p-server-cms"
  ],
  "homepage": "https://github.com/kokspflanze/PServerCMS",
  "autoload": {
    "psr-0": {
      "PServerCMS\\": "src/",
      "PServerCMSTest\\": "tests/"
