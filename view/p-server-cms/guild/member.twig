<div class="table-responsive">
	<table class="table table-striped table-hover">
		<thead>
		<tr>
			<th>#</th>
			<th>{{ translate('Character') }}</th>
			<th>{{ translate('Level') }}</th>
		</tr>
		</thead>
		{% set i = (member.getCurrentPageNumber() - 1) * member.getDefaultItemCountPerPage() + 1 %}
		{% for key, player in member %}
			<tr>
				<td>{{ i }}</td>
				<td><a href="{{ url('PServerCMS/character', {'id':player.getCharacter().getId()}) }}">{{ player.getCharacter().getName() }}</a></td>
				<td>{{ player.getLevel() }}</td>
			</tr>
			{% set i = i + 1 %}
		{% endfor %}
	</table>
</div>
{{ paginationControl(
	member,
	'Sliding',
	'p-server-cms/paginator',
		{'route' : 'guild', 'params' : {'id' : guild.getId, 'action' : 'member'}, 'ajax':'#pServerCMSGuildMember' }
) }}