{% extends 'layout/layout' %}

{% block title %}
	{{ translate('Guild') ~ ' - ' ~ guild.getName() }}
{% endblock title %}

{% block content %}
	<div class="table-responsive">
		<table class="table table-striped table-hover">
			<tr>
				<th>{{ translate('Guild') }}</th>
				<th>{{ guild.getName() }}</th>
			</tr>
			<tr>
				<th>{{ translate('Level') }}</th>
				<th>{{ guild.getLevel() }}</th>
			</tr>
		</table>
	</div>
{% endblock content %}